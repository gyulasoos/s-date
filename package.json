{
  "name": "s-date",
  "version": "1.4.1",
  "description": "Tiny date/time formatter",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "index.js",
    "test",
    "etc",
    "README.md"
  ],
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint",
    "lint": "jscs *.js --config=./etc/.jscs.json && eslint *.js -c etc/.eslint.json",
    "postlint": "npm run coverage",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -dir etc --report lcovonly -- test/ -R spec",
    "postcoverage": "rm etc/coverage.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansandqvist/s-date.git"
  },
  "keywords": [
    "date",
    "time",
    "format"
  ],
  "author": "sebastian sandqvist",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastiansandqvist/s-date/issues"
  },
  "homepage": "https://github.com/sebastiansandqvist/s-date",
  "devDependencies": {
    "@seabass/eslint-config": "^1.0.4",
    "chai": "^2.3.0",
    "codeclimate-test-reporter": "0.0.4",
    "eslint": "^1.3.1",
    "istanbul": "^0.3.19",
    "jscs": "^2.1.1",
    "mocha": "^2.2.5"
  }
}
